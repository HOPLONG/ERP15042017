                                        <select ng-model="arraytaikhoan[$index].loai_tai_khoan" class="form-control input-md" style="height: 100%;padding-left: 5px;">
                                            <option ng-repeat="loaitaikhoan in list_loaitaikhoan" value="{{loaitaikhoan.MA_LOAI}}">{{loaitaikhoan.TEN_LOAI}}</option>
                                        </select>
                                    </td>
                                </tr>
                                <tr>
                                    <td colspan='11' style='height: 25px'></td>
                                </tr>
                                <tr>
                                    <td colspan='11'><p style="text-shadow: 1px 1px 5px #91DDFD; font-style:italic; font-size: 20px;float:left; color: #64B5F6; margin-left:10px; margin-top:5px;margin-bottom:5px; font-weight: bold">LIÊN HỆ KHÁCH HÀNG</p></td>
                                </tr>
                                <tr>
                                     <th class="text-center" style="width:10%; background: #64B5F6;color: white;">Người liên hệ</th>
                                    <th class="text-center" style="width:10%; background: #64B5F6;color: white;">Chức vụ</th>
                                    <th class="text-center" style="width:10%; background: #64B5F6;color: white;">Phòng ban</th>
                                    <th class="text-center" style="width:10%; background: #64B5F6;color: white;">Ngày sinh</th>
                                    <th class="text-center" style="width:10%; background: #64B5F6;color: white;">Giới tính</th>
                                    <th class="text-center" style="width:10%; background: #64B5F6;color: white;">SĐT</th>
