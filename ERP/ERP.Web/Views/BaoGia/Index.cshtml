                                                                        <td colspan='11'><span ng-model="tong_khach_nhan_edit">{{tong_khach_nhan_edit | currency: ''}} vnd</span></td>
                                                                    </tr>
                                                                </table>

                                                            </td>
                                                        </tr>

                                                    </tfoot>
                                                </table>
