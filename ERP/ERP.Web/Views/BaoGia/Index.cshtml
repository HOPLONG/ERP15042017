                                                                    <tr>
                                                                        <td>
                                                                            <label class="control-label">Phương thức thanh toán </label>
                                                                        </td>
                                                                        <td>
                                                                            <select class="form-control select" ng-model="item.PHUONG_THUC_THANH_TOAN" id="adddieukhoanthanhtoan"
