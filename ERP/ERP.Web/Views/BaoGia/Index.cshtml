                                                            <td>
                                                                <div class="text-center">
                                                                    <center><i class="fa fa-minus-circle" style="color:red;" ng-click="Remove($index,item)" aria-hidden="true"></i></center>
                                                                </div>
                                                            </td>
                                                        </tr>
                                                    </tbody>
                                                    <tfoot>
                                                        <tr>
                                                            <td colspan='13' style="height:20px;"></td>
                                                        </tr>
