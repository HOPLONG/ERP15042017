                                                                    </tr>
                                                                    <tr>
                                                                        <td><label class="control-label">Tổng giá trị chênh lệch: </label></td>
                                                                        <td colspan='2'><span ng-model="gia_tri_chenh_lech_edit">{{gia_tri_chenh_lech_edit | currency: ''}} vnd</span></td>
